#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/wiremock-rest-client@1.11.0_encoding@0.1.13/node_modules/wiremock-rest-client/bin/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/wiremock-rest-client@1.11.0_encoding@0.1.13/node_modules/wiremock-rest-client/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/wiremock-rest-client@1.11.0_encoding@0.1.13/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/wiremock-rest-client@1.11.0_encoding@0.1.13/node_modules/wiremock-rest-client/bin/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/wiremock-rest-client@1.11.0_encoding@0.1.13/node_modules/wiremock-rest-client/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/wiremock-rest-client@1.11.0_encoding@0.1.13/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../wiremock-rest-client/bin/index.js" "$@"
else
  exec node  "$basedir/../wiremock-rest-client/bin/index.js" "$@"
fi
