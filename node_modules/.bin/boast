#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/swagger2openapi@7.0.8_encoding@0.1.13/node_modules/swagger2openapi/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/swagger2openapi@7.0.8_encoding@0.1.13/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/swagger2openapi@7.0.8_encoding@0.1.13/node_modules/swagger2openapi/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/swagger2openapi@7.0.8_encoding@0.1.13/node_modules:/Users/mathieudrimonakos/Documents/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../swagger2openapi/boast.js" "$@"
else
  exec node  "$basedir/../swagger2openapi/boast.js" "$@"
fi
